{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "timers": {
            "maxConcurrentExecutions": 1,
            "scheduleMonitorPollInterval": "00:00:10"
        }
    }
}